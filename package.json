{
  "name": "kingbayomoneyempire",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "AI-powered sports analytics platform for high-probability accumulator slips - Cold-blooded analytical predator hunting across global markets",
  "keywords": [
    "sports-analytics",
    "ai-predictions",
    "accumulator-bets",
    "react",
    "typescript",
    "pwa",
    "sports-betting",
    "gemini-ai",
    "kingbayomoneyempire"
  ],
  "author": "AariNAT Company Limited",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KobOmoba/Kingbayo_moneyempire.git"
  },
  "homepage": "https://kingbayomoneyempire.vercel.app",
  "bugs": {
    "url": "https://github.com/KobOmoba/Kingbayo_moneyempire/issues"
  },
  "scripts": {
    "dev": "vite --host --port 5000",
    "build": "tsc --noEmit && vite build",
    "build:analyze": "tsc --noEmit && vite build --mode analysis",
    "preview": "vite preview --host --port 5001",
    "preview:production": "vite preview --host --port 5001 --mode production",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "deploy": "npm run build && npx vercel --prod",
    "deploy:preview": "npm run build && npx vercel",
    "clean": "rm -rf dist node_modules/.vite",
    "build:stats": "npm run build -- --mode stats"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@tailwindcss/postcss": "^4.1.17",
    "autoprefixer": "^10.4.22",
    "lucide-react": "^0.263.1",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.4.18",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-core": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-checker": "^0.6.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "18.17.0"
  }
}
